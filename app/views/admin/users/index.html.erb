<%- if @user.present? -%>
  <table>
    <%- @user.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.remember_created_at %></td>
        <td><%= user.admin %></td>
      </tr>
    <%- end -%>
  </table>
<%- end -%>
